        <Button x:Name="addPay_btn" Content="Add Payment" Height="56" Margin="307,308,306,0" VerticalAlignment="Top" Click="addPay_click" Background="#FF918F8B" FontSize="24" Foreground="Black" HorizontalContentAlignment="Center"/>
        <Button x:Name="createAcc_btn" Content="Create Account" Height="56" Margin="307,160,306,0" VerticalAlignment="Top" Click="createAcc_click" Background="#FF918F8B" FontSize="24" Foreground="Black" HorizontalContentAlignment="Center"/>

    </Grid>
</Window>
