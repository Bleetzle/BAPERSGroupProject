<Window x:Class="Bapers.GUI.accountCreated_popup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bapers.GUI"
        mc:Ignorable="d"
        Title="accountCreated_popup" Height="297" Width="699" Background="#FFE4DCCF">
    <Grid Margin="0,0,0,-6">
        <TextBlock HorizontalAlignment="Left" Height="130" Margin="16,25,-21,0" TextWrapping="Wrap" Text="Account created, would you like to add jobs?" VerticalAlignment="Top" Width="661" FontSize="48" TextAlignment="Center"/>
        <Button x:Name="no_btn" Content="No" HorizontalAlignment="Left" Height="45" Margin="435,200,0,0" VerticalAlignment="Top" Width="175" Click="no_Click" Background="#FF918F8B" FontFamily="Segoe UI Semibold" FontSize="24" Foreground="Black"/>
        <Button x:Name="yes_btn" Content="Yes" HorizontalAlignment="Left" Height="45" Margin="70,200,0,0" VerticalAlignment="Top" Width="175" Click="yes_Click" Background="#FF918F8B" FontFamily="Segoe UI Semibold" FontSize="24" Foreground="Black"/>

    </Grid>
</Window>
